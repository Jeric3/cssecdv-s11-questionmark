<body>
	<h2>Admin Panel</h2>
	<div>
		<h3>Registered Users</h3>
		<ul id = "userList" class = "admin-user-list">
			{{#each users}}
				{{> adminUserList email=email id=id}}
			{{/each}}
		</ul>
	</div>
	<form action = "/logout" method = "POST">
		<button type = "submit">Logout</button>
	</form>
</body>